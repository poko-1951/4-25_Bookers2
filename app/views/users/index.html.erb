<%= render "show", user:  User.find(current_user.id) %>
<%= render "books/new", book: Book.new %>
<h2>Users</h2>
<table>
  <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
  </tr>
  <% @users.each do |user|%>
  <tr>
    <td><%= image_tag user.get_profile_image(80,80)%></td>
    <td><%= user.name%></td>
    <td><%= link_to "Show", user_path(user.id)%></td>
    <% end %>
  </tr>
</table>